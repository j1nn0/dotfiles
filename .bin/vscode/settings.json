{
  "Devdb.colorTheme": "dark",
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "accessibility.signals.chatEditModifiedFile": {
    "sound": "off"
  },
  "accessibility.signals.chatRequestSent": {
    "sound": "off"
  },
  "accessibility.signals.chatResponseReceived": {
    "sound": "off"
  },
  "accessibility.signals.clear": {
    "sound": "off"
  },
  "accessibility.signals.codeActionApplied": {
    "sound": "off"
  },
  "accessibility.signals.codeActionTriggered": {
    "sound": "off"
  },
  "accessibility.signals.diffLineDeleted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineInserted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineModified": {
    "sound": "off"
  },
  "accessibility.signals.editsKept": {
    "sound": "off"
  },
  "accessibility.signals.editsUndone": {
    "sound": "off"
  },
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off"
  },
  "accessibility.signals.lineHasError": {
    "sound": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.lineHasWarning": {
    "sound": "off"
  },
  "accessibility.signals.noInlayHints": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellCompleted": {
    "sound": "off"
  },
  "accessibility.signals.notebookCellFailed": {
    "sound": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off"
  },
  "accessibility.signals.positionHasError": {
    "sound": "off"
  },
  "accessibility.signals.positionHasWarning": {
    "sound": "off"
  },
  "accessibility.signals.progress": {
    "sound": "off"
  },
  "accessibility.signals.taskCompleted": {
    "sound": "off"
  },
  "accessibility.signals.taskFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalBell": {
    "sound": "off"
  },
  "accessibility.signals.terminalCommandFailed": {
    "sound": "off"
  },
  "accessibility.signals.terminalCommandSucceeded": {
    "sound": "off"
  },
  "accessibility.signals.terminalQuickFix": {
    "sound": "off"
  },
  "amazonQ.suppressPrompts": {
    "amazonQChatDisclaimer": true
  },
  "auto-close-tag.activationOnLanguage": [
    "xml",
    //"php",
    "blade",
    "ejs",
    "jinja",
    //"javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    //"plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "aws.telemetry": false,
  "debug.javascript.defaultRuntimeExecutable": {
    "pwa-node": "~/.local/share/mise/shims/node"
  },
  "editor.accessibilitySupport": "on",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "explicit",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'UDEV Gothic 35NFLG', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": "active",
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.showFoldingControls": "always",
  "editor.wordSegmenterLocales": ["ja"],
  "editor.wrappingIndent": "indent",
  "emmet.excludeLanguages": ["markdown", "php", "vue"],
  "explorer.compactFolders": false,
  "files.associations": {
    // shell-formatだとインデントが無くなるのでプレーンテキストにしておく
    ".gitconfig*": "plaintext"
  },
  "files.autoGuessEncoding": true,
  "files.candidateGuessEncodings": ["utf8", "shiftjis", "eucjp"],
  "git-graph.date.format": "ISO Date & Time",
  "git.allowNoVerifyCommit": true,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.views.repositories.showBranchComparison": false,
  "gitlens.views.repositories.showBranches": false,
  "gitlens.views.repositories.showCommits": false,
  "gitlens.views.repositories.showContributors": false,
  "gitlens.views.repositories.showRemotes": false,
  "gitlens.views.repositories.showStashes": true,
  "gitlens.views.repositories.showTags": false,
  "gitlens.views.repositories.showUpstreamStatus": false,
  "gitlens.views.repositories.showWorktrees": false,
  "hediet.vscode-drawio.resizeImages": null,
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "php.debug.executablePath": "~/.local/share/mise/shims/php",
  "php.validate.executablePath": "~/.local/share/mise/shims/php",
  "redhat.telemetry.enabled": true,
  "scm.compactFolders": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.external.osxExec": "Ghostty.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "zsh (2)",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "'UDEV Gothic 35NF', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "args": ["-l"],
      "icon": "terminal-bash",
      "path": "bash"
    },
    "bash (2)": {
      "path": "/opt/homebrew/bin/bash"
    },
    "fish": {
      "args": ["-l"],
      "path": "fish"
    },
    "pwsh": {
      "icon": "terminal-powershell",
      "path": "pwsh"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    },
    "zsh": {
      "args": ["-l"],
      "path": "zsh"
    },
    "zsh (2)": {
      "path": "/opt/homebrew/bin/zsh"
    }
  },
  "todo-tree.general.tagGroups": {
    "FIXME": ["FIXME", "FIXIT", "FIX"]
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "UNDONE"
  ],
  "workbench.colorTheme": "Rouge 2",
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "none",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.extension.enableComposeLanguageServer": false,
  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "workbench.editor.empty.hint": "hidden",
  "vite.autoStart": false,
  "git.enableSmartCommit": true,
  "workbench.editor.showTabIndex": true,
  "workbench.iconTheme": "file-icons",
  // Rougue 2 themes override
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#17182B",
    "titleBar.activeForeground": "#bfbfbf",
    "titleBar.inactiveBackground": "#17182B",
    "titleBar.inactiveForeground": "#777777"
  }
}
